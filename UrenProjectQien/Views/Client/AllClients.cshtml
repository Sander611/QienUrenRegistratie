@model IEnumerable<Shared.Models.Client>

@{
    ViewData["Title"] = "List of All Clients";
    
}

    <html>
    <head>
        <title></title>
    </head>
    <body>
        <table>
            <thead>
                <tr>
                    <th>Bedrijf</th>
                    <th>Contact Person 1</th>
                    <th>Contact Person 2</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @item.CompanyName
                        </td>
                        <td>
                            @item.ClientName1
                        </td>
                        <td>
                            @item.ClientName2
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </body>
    </html>